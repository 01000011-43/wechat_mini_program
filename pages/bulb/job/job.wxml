<!--pages/bulb/job/job.wxml-->

<view class="top">
  <view class="search">
    <view class="search_icon">
      <image class="search_img" src="/images/search.png"></image>
    </view>
    <view class="search_txt">输入查找内容</view>
  </view>
</view>

<block wx:for="{{list}}" wx:key="list">
  <view class="memo">
    <view class="memo_title">#实习{{item.id}}</view>
    <view class="memo_txt">{{item.content}}</view>
    <view class="memo_time">{{item.time}}</view>
  </view>
</block>