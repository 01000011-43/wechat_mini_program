}
.detail .detail_info .price_box .price_set .price_number_box .price_number {
  font-size: large;
  font-weight: bolder;
}
.detail .detail_info .price_box .price_set .price_txt {
  flex: 3;
}
.detail .detail_info .message {
  padding: 50rpx 0 20rpx 0;
  margin: 0 20rpx;
}
.picker {
  color: #828282;
}
.picker1 {
  color: #000000;
