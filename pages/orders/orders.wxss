page {
  background-color: #f4f4f4;
}
.copy_page {
  position: relative;
  top: 30rpx;
  display: flex;
  justify-content: center;
}
.copy_page .input_box {
  background-color: #fff;
  width: 700rpx;
  height: 100rpx;
  border-radius: 20rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.copy_page .input_box .input_txt {
  font-size: 32rpx;
}
.school_pick {
  position: relative;
  top: 50rpx;
  left: 28rpx;
  width: 300rpx;
  background-color: white;
  border-bottom-right-radius: 30rpx;
  border-top-right-radius: 30rpx;
}
.school_pick .school {
  display: flex;
  flex-direction: row;
  align-items: center;
}
.school_pick .school image {
  width: 50rpx;
}
.school_pick .school .school_name {
  font-size: 25rpx;
  padding: 0 0 0 20rpx;
}
.personal {
  position: relative;
  top: 30rpx;
  display: flex;
  justify-content: center;
}
.personal .person_info {
  background-color: #fff;
  width: 700rpx;
  border-radius: 20rpx;
}
.personal .person_info .person_info_title {
  display: flex;
  border-bottom: 1rpx solid #e0e0e0;
  flex-direction: row;
  align-items: center;
}
.personal .person_info .person_info_title .title_img {
  flex: 1;
  padding: 15rpx;
}
.personal .person_info .person_info_title .title_txt {
  flex: 9;
  font-size: large;
  font-weight: bold;
}
.personal .person_info .person_info_content .name_phone_address {
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}
.personal .person_info .person_info_content .name_phone_address .content {
  flex: 10;
  padding: 20rpx 20rpx 20rpx 40rpx;
}
.personal .person_info .person_info_content .name_phone_address .content .address_content {
  color: #828282;
}
.personal .person_info .person_info_content .name_phone_address .content .name_content {
  color: #828282;
}
.personal .person_info .person_info_content .name_phone_address .name_img {
  flex: 1;
  padding: 20rpx;
}
.update {
  background-color: #fff;
}
.update .update_info .update_title {
  display: flex;
  justify-content: center;
}
.update .update_info .update_title .title_info {
  font-size: larger;
  font-weight: bolder;
  padding: 20rpx;
}
.update .update_info .name_info {
  position: relative;
  top: 30rpx;
  left: 50rpx;
  padding: 10rpx 0;
}
.update .update_info .name_info .name_space {
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  padding: 10rpx 0;
  background-color: #fff;
}
.update .update_info .name_info .name_space .shoujianren {
  font-size: large;
  font-weight: bold;
  padding: 0 100rpx 0 0;
}
.update .update_info .name_info .name_space .shoujiandizhi {
  font-size: large;
  font-weight: bold;
  padding: 0 70rpx 0 0;
}
.update .update_info .name_info .name_space .truename {
  font-size: large;
}
.update .update_info .name_info .name_space .phone_title {
  flex: 3;
  font-size: large;
  font-weight: bold;
  padding: 0 70rpx 0 0;
}
.update .update_info .name_info .name_space .phone_number {
  flex: 1;
}
.update .update_info .name_info .name_space .get_phone {
  flex: 3;
  background-color: #fff;
  color: #55b6c5;
  font-size: 30rpx;
  padding: 0 100rpx 0 0;
}
.update .update_info .name_info .name_attention {
  display: flex;
  align-items: center;
  padding: 15rpx 0 45rpx 0;
}
.update .update_info .name_info .name_attention image {
  flex: 1;
  width: 70%;
  padding: 0 20rpx 0 0 ;
}
.update .update_info .name_info .name_attention .tips {
  flex: 20;
  color: #a0a0a0;
  font-size: small;
}
.update .update_info .name_info .name_attention .tips .school_tips {
  font-weight: bolder;
  font-size: medium;
}
.update_button {
  padding: 20rpx;
}
.detail {
